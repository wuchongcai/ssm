body,html{
    padding:0;
    margin: 0;
    border:none;
    width: 100%;
    height: 100%;
    list-style: none;
}

.breadcrumb{
    background: #F0F0F0;
    border-radius: 0;
}
.breadcrumb .icon-shouye{
    color: #319dfc;
    font-size: 13px;
    margin-right:3px;
    position: absolute;
    top:15px;
    left:20px;
}
.breadcrumb .icon-down-trangle1{
    font-size: 10px;
}
.breadcrumb {
    padding: 12px 14px 12px 40px;
}
.tit{
    width:95%;
    height:40px;
    line-height: 40px;
    font-size: 14px;
    color: white;
    margin: auto;
    background: #555555;
}
.tit .icon-guanlianniu{
    margin-left: 20px;
    font-size: 20px;
    font-weight: bold;
    margin-right: 10px;
}
.tit .icon-down-trangle2{
    font-size: 14px;
    font-weight: bold;
    float: right;
    margin-right: 10px;
    line-height: 45px;
}